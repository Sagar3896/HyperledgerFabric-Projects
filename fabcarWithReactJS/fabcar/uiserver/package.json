{
    "name": "fabcar",
    "version": "1.0.0",
    "BROWSER": "google-chrome-stable",
    "description": "FabCar application implemented in JavaScript",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "start": "concurrently \"npm run server\" \"cd ../client && npm start\"",
        "server": "nodemon uiserver.js",
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "nyc mocha --recursive",
        "browser": "google-chrome"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "body-parser": "^1.20.0",
        "buffer": "^6.0.3",
        "concurrently": "^7.2.1",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "crypto-browserify": "^3.12.0",
        "ejs": "^3.1.8",
        "express": "^4.18.1",
        "express-session": "^1.17.3",
        "fabric-ca-client": "^2.2.13",
        "fabric-network": "^2.2.13",
        "html": "^1.0.0",
        "path-browserify": "^1.0.1",
        "pug": "^3.0.2",
        "react-scripts": "^5.0.1",
        "sessionstorage": "^0.1.0",
        "sessionstorage-for-nodejs": "^1.0.2",
        "stream": "0.0.2",
        "url": "^0.11.0",
        "util": "^0.12.4",
        "webpack": "^4.19.1",
        "webpack-dev-server": "^4.10.0"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "eslint": "^5.9.0",
        "mocha": "^9.2.2",
        "nyc": "^14.1.1",
        "react-error-overlay": "^6.0.9",
        "sinon": "^7.1.1",
        "sinon-chai": "^3.3.0"
    },
    "resolutions": {
        "react-error-overlay": "6.0.9"
    },
    "browser": {
        "crypto": false,
        "stream": false
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
